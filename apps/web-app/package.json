{
  "name": "web-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular",
          "printWidth": 100,
          "tabWidth": 2,
          "useTabs": false,
          "embeddedLanguageFormatting": "off",
          "singleQuote": true,
          "semi": true,
          "quoteProps": "preserve",
          "bracketSpacing": false,
          "trailingComma": "all",
          "bracketSameLine": true
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.1.3",
    "@angular/common": "^20.1.3",
    "@angular/compiler": "^20.1.3",
    "@angular/core": "^20.1.3",
    "@angular/forms": "^20.2.1",
    "@angular/material": "^20.2.0",
    "@angular/platform-browser": "^20.1.3",
    "@angular/router": "^20.2.1",
    "@azure/msal-angular": "^4.0.19",
    "@azure/msal-browser": "^4.21.0",
    "angular-ecmascript-intl": "^6.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular/build": "^20.2.0",
    "@angular/cli": "^20.2.0",
    "@angular/compiler-cli": "^20.2.1",
    "@types/jasmine": "~5.1.8",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  },
  "engines": {
    "node": ">=20.19.0  <=24.3.0"
  }
}
